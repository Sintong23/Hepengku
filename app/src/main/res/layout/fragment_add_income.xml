<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddIncomeFragment">

    <ImageView
        android:id="@+id/iv_other"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="230dp"
        android:src="@drawable/other"
        app:layout_constraintStart_toStartOf="@+id/ivsalary"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_other"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Others"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_other"
        app:layout_constraintStart_toStartOf="@+id/iv_other"
        app:layout_constraintTop_toBottomOf="@+id/iv_other" />

    <TextView
        android:id="@+id/tv_bonus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bonus"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_bonus"
        app:layout_constraintStart_toStartOf="@+id/iv_bonus"
        app:layout_constraintTop_toBottomOf="@+id/iv_bonus" />

    <TextView
        android:id="@+id/tv_part_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Part-Time"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_part_time"
        app:layout_constraintStart_toStartOf="@+id/iv_part_time"
        app:layout_constraintTop_toBottomOf="@+id/iv_part_time" />

    <ImageView
        android:id="@+id/iv_bonus"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/bonus"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_part_time"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <ImageView
        android:id="@+id/iv_part_time"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/parttime"
        app:layout_constraintStart_toEndOf="@id/iv_invest"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintEnd_toStartOf="@id/iv_bonus"/>

    <ImageView
        android:id="@+id/iv_invest"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/stock"
        app:layout_constraintStart_toEndOf="@id/ivsalary"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintEnd_toStartOf="@id/iv_part_time"/>

    <TextView
        android:id="@+id/tv_investments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Investments"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_invest"
        app:layout_constraintStart_toStartOf="@+id/iv_invest"
        app:layout_constraintTop_toBottomOf="@+id/iv_invest" />

    <View
        android:id="@+id/view15"
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:background="#B5E550"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

    <View
        android:id="@+id/view8"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#8CD36C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="ADD"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintStart_toStartOf="@+id/view8"
        app:layout_constraintTop_toTopOf="@id/view8" />

    <View
        android:id="@+id/view14"
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:background="#ECECA3"
        app:layout_constraintEnd_toStartOf="@+id/view15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_income1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="INCOME"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/view15"
        app:layout_constraintEnd_toEndOf="@+id/view15"
        app:layout_constraintStart_toStartOf="@+id/view15"
        app:layout_constraintTop_toTopOf="@+id/view15" />

    <TextView
        android:id="@+id/tv_expansess"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EXPENSE"
        app:layout_constraintBottom_toBottomOf="@+id/view14"
        app:layout_constraintEnd_toEndOf="@+id/view14"
        app:layout_constraintStart_toStartOf="@+id/view14"
        app:layout_constraintTop_toTopOf="@+id/view14" />

    <ImageView
        android:id="@+id/ivsalary"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/salary"
        app:layout_constraintEnd_toStartOf="@+id/iv_invest"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_salary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salary"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/ivsalary"
        app:layout_constraintStart_toStartOf="@+id/ivsalary"
        app:layout_constraintTop_toBottomOf="@+id/ivsalary" />

    <FrameLayout
        android:id="@+id/custom_keyboard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
