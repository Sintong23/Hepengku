<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddExpensesFragment">

    <ImageView
        android:id="@+id/iv_furniture"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="45dp"
        android:src="@drawable/home"
        app:layout_constraintStart_toStartOf="@+id/iv_repair"
        app:layout_constraintTop_toBottomOf="@+id/tv_repair" />

    <TextView
        android:id="@+id/tv_furniture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Furniture"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_furniture"
        app:layout_constraintStart_toStartOf="@+id/iv_furniture"
        app:layout_constraintTop_toBottomOf="@+id/iv_furniture" />

    <ImageView
        android:id="@+id/iv_olahraga"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="350dp"
        android:src="@drawable/olahraga"
        app:layout_constraintEnd_toStartOf="@id/iv_gift"
        app:layout_constraintStart_toEndOf="@id/iv_donation"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sports"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_olahraga"
        app:layout_constraintStart_toStartOf="@+id/iv_olahraga"
        app:layout_constraintTop_toBottomOf="@+id/iv_olahraga" />

    <TextView
        android:id="@+id/tv_gift"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gift"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_gift"
        app:layout_constraintStart_toStartOf="@+id/iv_gift"
        app:layout_constraintTop_toBottomOf="@+id/iv_gift" />

    <ImageView
        android:id="@+id/iv_repair"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="350dp"
        android:src="@drawable/repair"
        app:layout_constraintEnd_toStartOf="@+id/iv_donation"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <ImageView
        android:id="@+id/iv_gift"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="350dp"
        android:src="@drawable/gift"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_olahraga"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_donation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Donation"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_donation"
        app:layout_constraintStart_toStartOf="@+id/iv_donation"
        app:layout_constraintTop_toBottomOf="@+id/iv_donation" />

    <ImageView
        android:id="@+id/iv_donation"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="350dp"
        android:src="@drawable/donation"
        app:layout_constraintEnd_toStartOf="@id/iv_olahraga"
        app:layout_constraintStart_toEndOf="@id/iv_repair"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_repair"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Repairs"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_repair"
        app:layout_constraintStart_toStartOf="@+id/iv_repair"
        app:layout_constraintTop_toBottomOf="@+id/iv_repair" />

    <ImageView
        android:id="@+id/iv_travelling"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="230dp"
        android:src="@drawable/traveling"
        app:layout_constraintEnd_toStartOf="@id/iv_transportation"
        app:layout_constraintStart_toEndOf="@id/iv_food"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <ImageView
        android:id="@+id/iv_clothing"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="230dp"
        android:src="@drawable/clothing"
        app:layout_constraintEnd_toStartOf="@+id/iv_food"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <ImageView
        android:id="@+id/iv_food"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="230dp"
        android:src="@drawable/food_"
        app:layout_constraintEnd_toStartOf="@id/iv_travelling"
        app:layout_constraintStart_toEndOf="@id/iv_clothing"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_travelling"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Travelling"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_travelling"
        app:layout_constraintStart_toStartOf="@+id/iv_travelling"
        app:layout_constraintTop_toBottomOf="@+id/iv_travelling" />

    <TextView
        android:id="@+id/tv_other"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clothing"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_clothing"
        app:layout_constraintStart_toStartOf="@+id/iv_clothing"
        app:layout_constraintTop_toBottomOf="@+id/iv_clothing" />

    <ImageView
        android:id="@+id/iv_transportation"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="230dp"
        android:src="@drawable/transportation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_travelling"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_transportation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Transport"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_transportation"
        app:layout_constraintStart_toStartOf="@+id/iv_transportation"
        app:layout_constraintTop_toBottomOf="@+id/iv_transportation" />

    <TextView
        android:id="@+id/tv_food"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Food"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_food"
        app:layout_constraintStart_toStartOf="@+id/iv_food"
        app:layout_constraintTop_toBottomOf="@+id/iv_food" />

    <ImageView
        android:id="@+id/iv_entertainment"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="468dp"
        android:src="@drawable/entertainment"
        app:layout_constraintEnd_toStartOf="@id/iv_health"
        app:layout_constraintStart_toEndOf="@+id/iv_furniture"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <ImageView
        android:id="@+id/iv_electronic"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="468dp"
        android:src="@drawable/electronic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/iv_health"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_entertainment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Entertainment"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_entertainment"
        app:layout_constraintStart_toStartOf="@+id/iv_entertainment"
        app:layout_constraintTop_toBottomOf="@+id/iv_entertainment" />

    <TextView
        android:id="@+id/tv_electronics"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Electronics"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_electronic"
        app:layout_constraintStart_toStartOf="@+id/iv_electronic"
        app:layout_constraintTop_toBottomOf="@+id/iv_electronic" />

    <ImageView
        android:id="@+id/iv_health"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="468dp"
        android:src="@drawable/health"
        app:layout_constraintEnd_toStartOf="@id/iv_electronic"
        app:layout_constraintStart_toEndOf="@+id/iv_entertainment"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_health"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Health"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_health"

        app:layout_constraintStart_toStartOf="@+id/iv_health"
        app:layout_constraintTop_toBottomOf="@+id/iv_health" />

    <TextView
        android:id="@+id/tv_bonus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pet"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_pet"
        app:layout_constraintStart_toStartOf="@+id/iv_pet"
        app:layout_constraintTop_toBottomOf="@+id/iv_pet" />

    <TextView
        android:id="@+id/tv_part_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Education"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_education"
        app:layout_constraintStart_toStartOf="@+id/iv_education"
        app:layout_constraintTop_toBottomOf="@+id/iv_education" />

    <ImageView
        android:id="@+id/iv_pet"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/pet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_education"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <ImageView
        android:id="@+id/iv_education"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/education"
        app:layout_constraintStart_toEndOf="@id/iv_car"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintEnd_toStartOf="@id/iv_pet"/>

    <ImageView
        android:id="@+id/iv_car"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/car"
        app:layout_constraintStart_toEndOf="@id/ivbeauty"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintEnd_toStartOf="@id/iv_education"/>

    <TextView
        android:id="@+id/tv_investments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Car"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_car"
        app:layout_constraintStart_toStartOf="@+id/iv_car"
        app:layout_constraintTop_toBottomOf="@+id/iv_car" />

    <View
        android:id="@+id/view15"
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:background="#ECECA3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_income1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="INCOME"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/view15"
        app:layout_constraintEnd_toEndOf="@+id/view15"
        app:layout_constraintStart_toStartOf="@+id/view15"
        app:layout_constraintTop_toTopOf="@+id/view15" />

    <View
        android:id="@+id/view8"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#8CD36C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvCancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Cancel"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="@+id/view8"
        app:layout_constraintTop_toTopOf="@id/view8" />

    <TextView
        android:id="@+id/tv_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="ADD"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintStart_toStartOf="@+id/view8"
        app:layout_constraintTop_toTopOf="@id/view8" />

    <View
        android:id="@+id/view14"
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:background="#B5E550"
        app:layout_constraintEnd_toStartOf="@+id/view15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_expansess"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EXPENSE"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/view14"
        app:layout_constraintEnd_toEndOf="@+id/view14"
        app:layout_constraintStart_toStartOf="@+id/view14"
        app:layout_constraintTop_toTopOf="@+id/view14" />

    <ImageView
        android:id="@+id/ivbeauty"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="110dp"
        android:src="@drawable/beuty"
        app:layout_constraintEnd_toStartOf="@+id/iv_car"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_salary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cosmetics"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/ivbeauty"
        app:layout_constraintStart_toStartOf="@+id/ivbeauty"
        app:layout_constraintTop_toBottomOf="@+id/ivbeauty" />

    <FrameLayout
        android:id="@+id/custom_keyboard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>